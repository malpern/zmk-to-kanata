/ {
    global {
        tap-time = <200>;
        hold-time = <250>;
    };

    keymap {
        compatible = "zmk,keymap";

        default_layer {
            bindings = <
                &kp A   &kp B   &kp C   &kp D
                &kp E   &kp F   &kp G   &kp H
                &kp I   &kp J   &kp K   &kp L
            >;
        };
    };
};